          <xsd:documentation>Material/Part number</xsd:documentation>
          <xsd:appinfo>
            <DataType>CHAR</DataType>
            <Length>35</Length>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="35"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="KTEXT" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Short text/description</xsd:documentation>
          <xsd:appinfo>
            <DataType>CHAR</DataType>
