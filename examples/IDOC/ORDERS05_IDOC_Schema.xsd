        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="15"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="PEINH" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Price unit</xsd:documentation>
          <xsd:appinfo>
            <DataType>DEC</DataType>
            <Length>9</Length>
            <Decimals>0</Decimals>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="9"/>
          </xsd:restriction>
        </xsd:simpleType>
