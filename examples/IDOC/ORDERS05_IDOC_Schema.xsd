        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="42"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="MFRNR" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Manufacturer number</xsd:documentation>
          <xsd:appinfo>
            <DataType>CHAR</DataType>
            <Length>10</Length>
