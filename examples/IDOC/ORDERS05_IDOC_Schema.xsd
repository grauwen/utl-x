          <xsd:documentation>Quantity in base UOM</xsd:documentation>
          <xsd:appinfo>
            <DataType>DEC</DataType>
            <Length>15</Length>
            <Decimals>3</Decimals>
          </xsd:appinfo>
        </xsd:annotation>
